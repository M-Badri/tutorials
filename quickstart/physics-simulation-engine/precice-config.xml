<?xml version="1.0"?>
<precice-configuration>
  <log>
    <sink
      filter="%Severity% > debug and %Rank% = 0"
      format="---[precice] %ColorizedSeverity% %Message%"
      enabled="true" />
  </log>

  <solver-interface dimensions="2">
    <data:vector name="Velocity" />

    <mesh name="Fluid-Mesh">
      <use-data name="Velocity" />
    </mesh>

    <mesh name="Controller-Mesh">
      <use-data name="Velocity" />
    </mesh>

    <participant name="Fluid">
      <use-mesh name="Fluid-Mesh" provide="yes" />
      <use-mesh name="Controller-Mesh" from="Controller" />
      <read-data name="Velocity" mesh="Fluid-Mesh" />
      <mapping:nearest-neighbor
        direction="read"
        from="Controller-Mesh"
        to="Fluid-Mesh"
        constraint="consistent" />
    </participant>

    <participant name="Controller">
      <use-mesh name="Controller-Mesh" provide="yes" />
      <use-mesh name="Fluid-Mesh" from="Fluid"/>
      <write-data name="Velocity" mesh="Controller-Mesh" />
    </participant>

    <m2n:sockets from="Controller" to="Fluid" exchange-directory="/data/ms2137/works/gymprecice_test/gymprecice-tutorials/closed_loop_AFC/jet_control_cylinder_2d/gymprecice-run/jet_cylinder_2d_controller_training_12042023_223129/precice-exchange-directory-0"  />

    <coupling-scheme:parallel-explicit>
      <max-time value="5.0" />
      <time-window-size value="0.0005" valid-digits="8" />
      <participants first="Controller" second="Fluid"/>
      <exchange data="Velocity" mesh="Controller-Mesh" from="Controller" to="Fluid" initialize="False"/>
    </coupling-scheme:parallel-explicit>
  </solver-interface>
</precice-configuration>